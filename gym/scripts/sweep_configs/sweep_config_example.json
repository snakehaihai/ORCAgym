{
    "method": "random", // 优化方法，"random" 表示使用随机搜索方法
    "metric": { // 评估指标配置
        "name": "Train/mean_reward", // 指标名称，这里是训练过程中的平均奖励
        "goal": "maximize" // 优化目标，这里是最大化平均奖励
    },
    "run_cap": 10, // 运行上限，表示最多进行10次实验运行
    "parameters": { // 超参数配置
        "train_cfg.runner.max_iterations": { // 训练配置中运行器的最大迭代次数
            "values": [10] // 该参数的可选值列表，这里固定为10次迭代
        },
        "env_cfg.env.episode_length_s": { // 环境配置中每个回合的时长（秒）
            "min": 2, // 回合时长的最小值，2秒
            "max": 10 // 回合时长的最大值，10秒
        },
        "train_cfg.policy.reward.weights.lin_vel_z": { // 训练配置中策略奖励的线速度Z轴权重
            "min": 0.1, // 权重的最小值，0.1
            "max": 1.0 // 权重的最大值，1.0
        },
        "env_cfg.env.num_envs": { // 环境配置中并行环境的数量
            "values": [64] // 该参数的可选值列表，这里固定为64个环境
        }
    }
}
